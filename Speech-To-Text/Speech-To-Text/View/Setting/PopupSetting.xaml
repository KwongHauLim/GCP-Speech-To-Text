<Window x:Class="Speech_To_Text.View.Setting.PopupSetting"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Speech_To_Text.View.Setting"
        mc:Ignorable="d" ShowInTaskbar="False"
        Title="Setting" Height="450" Width="400" Icon="/Speech-To-Text;component/Resources/Images/Home.png"
        Closing="Window_Closing">
    <Grid>
        <TabControl x:Name="uiTabs">
            <TabItem Header="General">
                <Grid Margin="5">
                    <local:UC_GeneralSetting x:Name="ucGeneral"/>
                </Grid>
            </TabItem>
            <TabItem Header="Google">
                <Grid Margin="5">
                    <local:UC_GoogleCredential x:Name="ucGoogle"/>
                </Grid>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
